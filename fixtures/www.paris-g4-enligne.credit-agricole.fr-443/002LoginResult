<SCRIPT LANGUAGE="JavaScript">
//surcharge de la fonction fwkPUAvancer pour pouvoir appeler des formulaires différents
function fwkPUAvancerForm(prm_action, formulaire) {	
	var frmFWK;
	frmFWK = document.forms[formulaire];
	frmFWK.fwkcodeaction.value = 'Executer';
	frmFWK.fwkaction.value = prm_action;
	frmFWK.submit();
}
</SCRIPT>

