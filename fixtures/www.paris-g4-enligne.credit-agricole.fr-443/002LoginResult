function getUrlAction(action,idZone) {
	var url = urlBase;
	url = url + "?sessionSAG="+idSessionSag;
	url = url + "&stbpg="+idPage;
	url = url + "&act="+action;
	if (idZone)
		url = url + "&stbzn="+idZone;
		
	return url;
}

function getUrlActionPartenaire(partenaire) {
	var url = urlBase;
	url = url + "?sessionSAG="+idSessionSag;
	url = url + "&stbpg="+idPage;
	url = url + "&site="+partenaire;
	url = url + "&typeaction=reroutage_aller";
	return url;
}

function doAction(act,idZone, actCrt, idCpt) {
	var url = getUrlAction(act,idZone);
	
	if (idZone == "bnt") {
		// action courant = action demandee
		var crt = act;
		// on surchage l'action courante si besoin (cas du detail)
		if (actCrt) {
			crt = actCrt;
		}
	
		indEt=crt.indexOf("&");
		if (indEt != -1)
			crt = crt.substring(0,indEt);

		url = url + "&actCrt="+crt;
		if (idCpt) 
			url = url + "&idCptCrt="+idCpt;
	}	
	document.location.href = url;
}

function doActionDelayed(act,idZone, actCrt, idCpt) {
	var html = " <SCRIPT language=\"JavaScript\" src=\"/web/bam/appli/web/commun/js/prisesFWK.js\" ></SCRIPT> <SCRIPT language=\"JavaScript\" src=\"/web/bam/appli/web/commun/js/outils.js\" > </SCRIPT> <SCRIPT language=\"JavaScript\" src=\"/web/bam/appli/web/commun/js/infosbulle.js\" > </SCRIPT> <table width=\"480\" border=\"0\" cellspacing=\"0\" cellpadding=\"0\"> <tr> <td colspan=\"2\" height=\"6\"></td> </tr> <tr> <td height=\"20\"><font class=\"titrepage\">Synthèse globale</font></td> <td height=\"20\"></td> </tr> <TR> <TD colSpan = \"2\" height=\"4\"><IMG src=\"/web/bam/appli/web/commun/picts/filettitrepage.gif\" border=0 width=\"100%\" height=\"1\"> </TD> </TR> </table> <br> <br> <FONT CLASS=\"taille3\" FACE=\"Helvetica,Arial\" SIZE=\"3\" color=\"Black\"> Veuillez patienter quelques instants pendant la construction de votre synthèse (cette opération peut prendre quelques minutes). </FONT><span id=\"infoBull\"></span><span id=\"ombrBull\"></span><br> <TABLE WIDTH=\"364\" BORDER=\"0\" CELLSPACING=\"0\" CELLPADDING=\"0\"> </TABLE> ";
	
	document.getElementById('trPagePu').innerHTML = html;
	
