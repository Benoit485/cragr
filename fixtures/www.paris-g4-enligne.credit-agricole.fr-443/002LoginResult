}

function enchainerApplication(idSite) {
	var url = getUrlAction('partenaire');
	if (idSite.substring(0,7) != "http://" && idSite.substring(0,8) != "https://")
		idSite = "http://"+idSite;
	url = url + "&cible="+idSite;
	window.open(url, "win", "height=500, width=600, location=yes, menubar=yes, resizable=yes, scrollbars=yes, status=yes, toolbar=yes");
}

function lancerURLParam(url,height,width,scrollbar){
	if (url.substring(0,7) != "http://" && url.substring(0,8) != "https://")
		url = "http://"+url;
	ouvrirPopup(url, "win", height, width, 'yes', 'yes','yes','yes','no',scrollbar);
}

function lancerURL(url){
	lancerURLParam(url,500,600,'yes');
}

function lancerURLSimul(url){
	lancerURLParam(url,500,690,'yes');
}

function ouvrirPopup(url, nom, h, w, loc, menu, tool, rsize, status, scroll){
window.open(url, nom, "height="+h+", width="+w+", location="+loc+", menubar="+menu+", resizable="+rsize+", scrollbars="+scroll+", status="+status+", toolbar="+tool);
}


