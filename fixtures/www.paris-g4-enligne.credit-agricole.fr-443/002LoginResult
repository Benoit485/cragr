function lancerSTEvCom(idPu, param1, obj, param2) {
	var aRemplacer = /\&/g;
	param2 = param2.replace(aRemplacer, "etcommercial")
	
	var url = getUrlAction(idPu);
	url += "&"+ param1;
	url += "&IDCOMP="+ getIdComp(obj);
	url += "&"+ param2;
	
	if (param2.substring(0,16) == "ACTION=lancerURL")
		ouvrirPopup(url,"win", 500, 600, 'yes', 'yes','yes','yes','no','yes');
	else
		document.location.href = url;
}

function lancerSTTrackingElargi(idPu, idCompSource, idCompCible, action) {
	var aRemplacer = /\&/g;
	action = action.replace(aRemplacer, "etcommercial")
	
	var url = getUrlAction(idPu);
	url += "&idCompSource="+ idCompSource;
	url += "&idCompCible="+ idCompCible;
	url += "&"+ action;
	url += "&provenance=TE";
	
	if (action.substring(0,16) == "ACTION=lancerURL")
		ouvrirPopup(url,"win", 500, 600, 'yes', 'yes','yes','yes','no','yes');
	else
		document.location.href = url;
	
}
function StatusMessage(over,msg) {
	if (browserOK==true) {
		if(over==true) window.status=msg;
		else window.status=" "; 
	}
}

