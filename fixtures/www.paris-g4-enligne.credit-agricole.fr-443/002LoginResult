var urlTelechargement= "";
function telechargeFinal() {
	return urlTelechargement;
	titre = "Téléchargement";
	aazz = "menubar=no,resizable=yes,toolbar=no,width=400,height=450";
	//popup1 = window.open(urlTelechargement,titre,aazz);
}
function ouvreTelechargement(val, isForm) {
	titre = "Téléchargement";
	var is_chrome = navigator.userAgent.toLowerCase().indexOf('chrome') > -1;
	if (is_chrome)  
	aazz = "menubar=no,resizable=yes,toolbar=no,width=860,height=480";
	else aazz = "menubar=no,resizable=yes,toolbar=no,width=520,height=480";
	//aazz = "menubar=no,resizable=yes,toolbar=no,width=520,height=480";
	popup1 = window.open('',titre,aazz);
	if (!isForm){
		url = val + "&typeaction=telechargement";
		urlTelechargement = url;
		var frmTel;
		frmTel = document.FRM_TEL;
		frmTel.urlTel.value = url;
		frmTel.target = 'Téléchargement';
		frmTel.submit();
	}
	if ((browserOK) && (navigator.appName=="Netscape")) popup1.focus();
	// Cas du telechargement a partir d un releve
	// Si modification du texte du pop-up de telechargement, penser a le modifier egalement dans Telechargement.jsp (cas a partir de la bnt ou de la synthese)
	if (isForm){
 		popup1.document.writeln('<html><head><title>'+titre+'</title>');
 		//on inclue toutes les css necessaires (cf enteteCSS.jsp)
 		popup1.document.writeln('<LINK REL="STYLESHEET" TYPE="text/css" HREF="'+FEUILLE_STYLE_ANTIQUUS+'">');
 		popup1.document.writeln('<LINK REL="STYLESHEET" TYPE="text/css" HREF="'+FEUILLE_STYLE_ANTIQUUS2+'">');
 		popup1.document.writeln('<LINK REL="STYLESHEET" TYPE="text/css" HREF="'+FEUILLE_STYLE+'">');
 		popup1.document.writeln('<LINK REL="STYLESHEET" TYPE="text/css" HREF="'+FEUILLE_STYLE2+'">');
 		popup1.document.writeln('<LINK REL="STYLESHEET" TYPE="text/css" HREF="'+FEUILLE_STYLE_MOD+'">'); 		
