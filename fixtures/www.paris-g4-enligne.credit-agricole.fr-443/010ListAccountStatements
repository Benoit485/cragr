function getIdComp(obj) {

  var cArret = 'BODY';
  var cNode = 'DIV';
  var cAttr = 'idcomp';
  
  if(obj.parentNode) {
  	var currNode = obj.parentNode;
  	var idcomp;
  	while(currNode && currNode.nodeName != cArret) {
    	if(currNode.nodeName == cNode && (idcomp = currNode.getAttribute(cAttr)))
      		return idcomp;
    
    	currNode = currNode.parentNode;
  	}
  }
  else {
	idcomp = obj;
	return idcomp;
  }
  return null;
}

function lancerPuParamComp(idPu, param, obj) {

  var url = getUrlAction(idPu);
  url += "&"+ param;
  url += "&IDCOMP="+ getIdComp(obj);
  
  document.location.href = url;
}

