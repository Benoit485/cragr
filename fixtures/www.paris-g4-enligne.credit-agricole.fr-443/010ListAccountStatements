var urlTelechargement= "";
function telechargeFinal() {
	return urlTelechargement;
	titre = "Téléchargement";
	aazz = "menubar=no,resizable=yes,toolbar=no,width=400,height=450";
	//popup1 = window.open(urlTelechargement,titre,aazz);
