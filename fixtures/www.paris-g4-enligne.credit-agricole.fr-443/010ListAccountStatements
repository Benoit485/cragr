<SCRIPT LANGUAGE="JavaScript">

// on recupere la version du navigateur
Version=navigator.appVersion
Version=Version.substring(0,1)
// ---------------------------Debut Infobulle utilisé pour firefox2 uniquement
IB = new Object;
var posX=0;posY=0;
var xOffset=10;
var yOffset=10;
var myTimer;
var urlNonModif = this.window.location.href;

function getFF2(it){
	if(navigator.appCodeName=="Mozilla"){
		var ffVers = navigator.userAgent.lastIndexOf('/');
		if(navigator.userAgent.substring(ffVers+1,ffVers+2)=="2"){
			it.title = " ";
			myTimer = setTimeout("showInfobulle('Votre document ne sera plus disponible. Si vous souhaitez <BR> conserver ce document, pensez à le télécharger.')",1000);
		}
	}
}
function showInfobulle(texte) {
	contenu="<TABLE bgcolor='"+IB.ColContour+"' border=0 cellspacing=0 cellpadding="+IB.NbPixel+"><TR><TD><TABLE border=0 cellpadding=2 cellspacing=0 bgcolor='"+IB.ColFond+"'><TR><TD><FONT size='-1' face='arial' color='"+IB.ColTexte+"'>"+texte+"</FONT></TD></TR></TABLE></TD></TR></TABLE>";
	var finalPosX=posX-xOffset;
	if (finalPosX<0) finalPosX=0;
	if (document.getElementById) {
		document.getElementById('bulle').innerHTML=contenu;
		document.getElementById('bulle').style.top=posY+yOffset;
		document.getElementById('bulle').style.left=finalPosX;
		document.getElementById('bulle').style.visibility="visible";
		setTimeout("hideBulle()",6000);
	}
}
function getMousePos(e) {
	posX=e.pageX;
	posY=e.pageY; 
}

function hideBulle() {
	if(navigator.appCodeName=="Mozilla"){
		var ffVers = navigator.userAgent.lastIndexOf('/');
		if(navigator.userAgent.substring(ffVers+1,ffVers+2)=="2"){
			if (document.getElementById){
				document.getElementById("bulle").style.visibility="hidden";
			}
		}
	}
}

function initBulle(ColTexte,ColFond,ColContour,NbPixel) {
	// Definie les couleur de l'infobulle
	IB.ColTexte=ColTexte;
	IB.ColFond=ColFond;
	IB.ColContour=ColContour;
	IB.NbPixel=NbPixel;
	if (document.getElementById) {
	    document.onmousemove=getMousePos;
	    document.write("<DIV id='bulle' style='border:1px solid black;position:absolute;top:0;left:0;visibility:hidden;z-index:9;'></DIV>");
	}
}
// --------------------------- Fin Infobulle pour firefox 2

// si c'est Netscape 2 ou plus, c'est bon
browserOK=((navigator.appCodeName=="Mozilla") && (Version > 1))

//Message affiche quand la souris est au-dessus: 
statusplan="Accès au plan du site";
statuslexique="Les principales notices bancaires";
statuscontact="Communiquez avec le Crédit Agricole";
statusaide="Besoin d'Aide ?";
statusrecherche="Rechercher un document électronique.";

arrPanneaux = new Array();
arrLibs = new Array();
arrBlocNewDocs = new Array();


totDoc = 0;    
indice = 0;    
nbDocPage = 0; 
pageCourante = 1;

tmpIdNiv4bis = "";

indDoubleClic = 0;
id2=0;
var lien2;
	
// Fonction de dépliage repliage de la synthèse dépliée
function showHideDeplie(id, entete){
	var elt = document.getElementById(id);
	if (elt != null){
		if (elt.style.display == "none"){
			elt.style.display = "block";
			document.getElementById(entete).src = "/web/bam/appli/web/commun/img/fleche_blanc_bas.png";
			document.getElementById(entete).style.display = "";
		}else{
			elt.style.display = "none";
			document.getElementById(entete).src = "/web/bam/appli/web/commun/img/fleche_blanc_droite.png";
			document.getElementById(entete).style.display = "";
		}
	}
	for (i=0;i<arrPanneaux.length;i++){
		var numImg = i+1;
		if(arrPanneaux[i] != id){
			document.getElementById(arrPanneaux[i]).style.display = "none";
			document.getElementById("imagefleche"+numImg).src = "/web/bam/appli/web/commun/img/fleche_blanc_droite.png";
			document.getElementById("Aimagefleche"+numImg).title = "Ouvrir le panneau "+arrLibs[i];
		}else{
			if(document.getElementById("Aimagefleche"+numImg).title == "Ouvrir le panneau "+arrLibs[i]){
				document.getElementById("Aimagefleche"+numImg).title = "Fermer le panneau "+arrLibs[i];
			}else{
				document.getElementById("Aimagefleche"+numImg).title = "Ouvrir le panneau "+arrLibs[i];
			}
		}
	}
	// On ferme le sous-niveau
	if(document.getElementById("ligne0") != null){
		if(document.getElementById("pagination")!= null){
			// Maj de la pagination
			indice = 0;
			pageCourante = Math.ceil((indice+1)/nbDocPage);
			dernierePage = Math.ceil(totDoc/nbDocPage);
			document.getElementById("page").innerHTML = pageCourante+' sur '+dernierePage;
		}
		document.getElementById("ligne0").style.display = "";
		if(document.getElementById("img-panneau-docs")!=null){
			document.getElementById("img-panneau-docs").src = "/web/bam/appli/web/commun/img/fleche_bleuc_droite.png";
			var titre = document.getElementById("title-panneau-docs").title;
			document.getElementById("title-panneau-docs").title = "Ouvrir "+titre.substring(7);
		}else{
			document.getElementById("niv4bisImg"+tmpIdNiv4bis).src = "/web/bam/appli/web/commun/img/fleche_blanc_droite.png";
			var titre = document.getElementById("niv4bisA"+tmpIdNiv4bis).title;
			document.getElementById("niv4bisA"+tmpIdNiv4bis).title = "Ouvrir "+titre.substring(7);
		}
		for(i=1;i<totDoc;i++){
			document.getElementById("ligne"+i).style.display = "none";
		}
		if(document.getElementById("pagination") != null){
			document.getElementById("pagination").style.display = "none";
			document.getElementById("prem").style.display = "none";
			document.getElementById("prec").style.display = "none";
			document.getElementById("suiv").style.display = "";
			document.getElementById("der").style.display = "";
		}
	}
}

// Fonction de dépliage repliage de la synthèse détail en cours
function showhideDocs(libPanneau,idALink,idImage){
	action = document.getElementById(idALink).title.substring(0,6);
	image = "";
	if(idImage == "img-panneau-docs"){
		image = "/web/bam/appli/web/commun/img/fleche_bleuc_droite.png";
	}else{
		image = "/web/bam/appli/web/commun/img/fleche_blanc_droite.png";
	}
	if(action == "Fermer"){
		indice = 0;
		document.getElementById(idImage).src = ""+image;
		document.getElementById(idALink).title = "Ouvrir le panneau "+libPanneau;
		if(document.getElementById("pagination")!= null){
			document.getElementById("pagination").style.display = "none";
			document.getElementById("prem").style.display = "none";
			document.getElementById("prec").style.display = "none";
			document.getElementById("suiv").style.display = "";
			document.getElementById("der").style.display = "";
			pageCourante = Math.ceil((indice+1)/nbDocPage);
			dernierePage = Math.ceil(totDoc/nbDocPage);
			document.getElementById("page").innerHTML = pageCourante+' sur '+dernierePage;
		}
	}else{
		if(idImage == "img-panneau-docs"){
			image = "/web/bam/appli/web/commun/img/fleche_bleuc_bas.png";
		}else{
			image = "/web/bam/appli/web/commun/img/fleche_blanc_bas.png";
		}
		document.getElementById(idImage).src = ""+image;
		document.getElementById(idALink).title = "Fermer le panneau "+libPanneau;
		if(document.getElementById("pagination")!= null){
			document.getElementById("pagination").style.display = "";
		}
	}
	document.getElementById("ligne0").style.display = "";
	for(i=1;i<totDoc;i++){
		if(action == "Fermer"){
			document.getElementById("ligne"+i).style.display = "none";
		}else{
			if(i<nbDocPage){
				document.getElementById("ligne"+i).style.display = "";
			}
		}
	}
}

// Fonction deplie vide
function deplie(){
}

// Fonction permettant de paginer la synthèse détail
function pagine(monid){
	if(monid=="prem"){
		indice = 0;
	}else if(monid=="der"){
		indice = totDoc - totDoc % nbDocPage;
		if(totDoc % nbDocPage == 0){
			indice -= nbDocPage;
		}
	}else if(monid=="suiv"){
		indice += nbDocPage;
	}else if(monid=="prec"){
		indice -= nbDocPage;
	} 
	for(i = 0; i<totDoc;i++){
		if(i>=indice && i<indice+nbDocPage){
			document.getElementById("ligne"+i).style.display = "";
		}else{
			document.getElementById("ligne"+i).style.display = "none";
		}
	}
	if(indice<nbDocPage){
		document.getElementById("prem").style.display = "none";
		document.getElementById("prec").style.display = "none";
		document.getElementById("suiv").style.display = "";
		document.getElementById("der").style.display = "";
	}else if(indice+1>totDoc-nbDocPage){
		document.getElementById("prem").style.display = "";
		document.getElementById("prec").style.display = "";
		document.getElementById("suiv").style.display = "none";
		document.getElementById("der").style.display = "none";
	}else{
		document.getElementById("prem").style.display = "";
		document.getElementById("prec").style.display = "";
		document.getElementById("suiv").style.display = "";
		document.getElementById("der").style.display = "";
	}
	pageCourante = Math.ceil((indice+1)/nbDocPage);
	dernierePage = Math.ceil(totDoc/nbDocPage);
	document.getElementById("page").innerHTML = pageCourante+' sur '+dernierePage;
}

function deplie(){
	// Permet au href du niveau4 de ne pas renvoyé en haut de la page.
}
   
function doubleClic(id){
 
	// recuperation du lien en cours
	var lien = document.getElementById(id).getAttribute("href");	
	
	// desactivation du lien en cours
	document.getElementById(id).removeAttribute("href");
	
	if (indDoubleClic==0){
		indDoubleClic = 1;
		id2=id;
		lien2=lien;
	}
	else
	{	 
		// ré-activation du lien précédent
		document.getElementById(id2).setAttribute("href",lien2);
		id2=id;
		lien2=lien;
	}
}

// document.getElementById("imgQEDNLU").style.display = "none";
// Fonction qui met à jour la date de consultation et enleve l'icone nouveau document
function majDateConsultation(id){

	date = new Date();
	idString = id.toString();
	numBloc = idString.charAt(0);
	
	jour = date.getDate().toString();
	if(jour.length<2) jour = "0" + jour;
	mois = (date.getMonth() + 1).toString();
	if(mois.length<2) mois = "0" + mois;
	
	myDate = jour+'/'+mois+'/'+date.getFullYear();
	document.getElementById("date"+id).innerHTML = myDate;
	
	if(document.getElementById("pict"+id)!= null){
		if(document.getElementById("pict"+id).style.display == ""){
			document.getElementById("pict"+id).style.display = "none";
			arrBlocNewDocs[numBloc] = arrBlocNewDocs[numBloc] - 1;
			if(arrBlocNewDocs[numBloc] == 0){
				document.getElementById("imgBloc"+numBloc).style.display = "none";
				
			}
		}
	}
	
	nbNew = 0;
	for (nbBlocs=1;nbBlocs<=arrBlocNewDocs.length;nbBlocs++){
		if (arrBlocNewDocs[nbBlocs] != null){
			nbNew = nbNew + arrBlocNewDocs[nbBlocs];
		}
	}
	if (nbNew == 0 && document.getElementById("imgQEDNLU") != null){
		document.getElementById("imgQEDNLU").style.display = "none";
	}
}

function ancre(){
	var ancre = "null";
	if(ancre != null){
		this.window.location.href = urlNonModif+'#'+ancre;
	}
}

function mettreUnCookie(nom, valeur) {
		//alert('nom: ' + nom + ' ; valeur: ' + valeur);
		var today = new Date();
		var expdate = new Date(); // crée un objet date indispensable 
		expdate = new Date(today.getTime() + ( 24 * 60 * 60 * 1000)); 
		// on rajoute 1 jour en millisecondes 
		document.cookie = "cookedoc"+nom + "=" + escape(valeur) + ((expdate == null)?"":("; expires=" + expdate.toGMTString())) + ";path=/";
}
function setCookie(c_name,value,expiredays)
{
var exdate=new Date();
exdate.setDate(exdate.getDate()+expiredays);
document.cookie=c_name+ "=" +escape(value)+
((expiredays==null) ? "" : ";expires="+exdate.toUTCString()) + ";path=/";
}

//fdn 


function activerLiens() {
	var page = document.getElementById("e-doc");
  	var tousLiens = page.getElementsByTagName("a");
	for (var i = 0; i < tousLiens.length; i++) {
   		var href = tousLiens[i].getAttribute("href_bak");
   		var srctab = tousLiens[i].getElementsByTagName("img");
   		if (href != null && href.indexOf('ouvreTelechargement')  != -1){   
    			tousLiens[i].setAttribute("href",href);
    			tousLiens[i].removeAttribute("href_bak");
    			if(srctab[0].getAttribute("src").indexOf('ajax') != -1){
    				srctab[0].removeAttribute("src");
    				srctab[0].setAttribute("src",srctab[0].getAttribute("src_bak"));
    				srctab[0].removeAttribute("src_bak");
    				srctab[0].removeAttribute("title");
    				srctab[0].setAttribute("title",srctab[0].getAttribute("title_bak"));
    				srctab[0].removeAttribute("title_bak");
       			}
    	}	
  	}
  	
}
    
function selectionLiens (id){
	var lien = document.getElementById(id).getAttribute("href");
	if (lien != null){
		document.getElementById(id).removeAttribute("href");
		document.getElementById(id).setAttribute("href_bak", lien);
		var srctab = document.getElementById(id).getElementsByTagName("img");
		srctab[0].setAttribute("src_bak", srctab[0].getAttribute("src"));
		srctab[0].removeAttribute("src");
   		srctab[0].setAttribute("src","/web/bam/appli/web/commun/img/ajax-loader.gif");
   		srctab[0].setAttribute("title_bak", srctab[0].getAttribute("title"));
   		srctab[0].removeAttribute("title");
   		srctab[0].setAttribute("title","Téléchargement en cours");
	}
    var page = document.getElementById("e-doc");
    var allAnchors = page.getElementsByTagName("a");
    for (var i = 0; i < allAnchors.length; i++) {
			if (allAnchors[i].getAttribute("href") != null && allAnchors[i].getAttribute("href").indexOf('ouvreTelechargement') != -1 && allAnchors[i].getAttribute("href_bak") == null) {
		   		allAnchors[i].setAttribute("href_bak", allAnchors[i].getAttribute("href"));
   				allAnchors[i].removeAttribute("href");
   			}		
    }
    
   var delaiEdoc = 30000;
   setTimeout("activerLiens()",delaiEdoc); 
}

//fin fdn


</SCRIPT>
