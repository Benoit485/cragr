function submitTelecharger(action) {
	var frm;
	frm = document.frm_tel;
	frm.typeaction.value = "telechargement";
	if (isIE55)	{
		frm.target = 'frameTelecharge';
	}
	frm.fwkcodeaction.value = 'Executer';
	frm.fwkaction.value = action;
	frm.submit();
}

function getCSS() {
	elt =  document.getElementById("linkcss");
	return elt.getAttribute("href");
}

function getCSSIE(num){
	var nom = navigator.appName;
	var version = navigator.appVersion;
	var retour = "";
	if(nom == "Microsoft Internet Explorer"){
		version = (version.split(";")[1]).split("\ ")[2].substring(0,1);
		var versionNum = parseInt(version);	
		var chemin = "/web/bam/appli/web/commun/styles/";
		if(num=="1"){
			chemin = "/web/bam/tech/allmedia/stb/commun/styles/";
		}
		if(versionNum == 8){
			retour = ""+chemin+"ie8.css?v=58";
		}else if(versionNum<= 7){
			retour = ""+chemin+"ie.css?v=58";			
		}		
	}
	return retour;
}

function getCSSIEOLD(){
	var nom = navigator.appName;
	var version = navigator.appVersion;
	var retour = "";
	if(nom == "Microsoft Internet Explorer"){
		version = (version.split(";")[1]).split("\ ")[2].substring(0,1);	
		var versionNum = parseInt(version);	
		if(versionNum< 7){
			retour = "/web/bam/tech/allmedia/stb/commun/styles/ieold.css?v=58";			
		}		
	}
	return retour;
}

